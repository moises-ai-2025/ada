#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/moisesgomes/Desktop/bolt.diy - cópia/bolt.diy/node_modules/.pnpm/vite-node@2.1.9_@types+node@22.13.1_sass-embedded@1.83.4/node_modules/vite-node/node_modules:/Users/moisesgomes/Desktop/bolt.diy - cópia/bolt.diy/node_modules/.pnpm/vite-node@2.1.9_@types+node@22.13.1_sass-embedded@1.83.4/node_modules:/Users/moisesgomes/Desktop/bolt.diy - cópia/bolt.diy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/moisesgomes/Desktop/bolt.diy - cópia/bolt.diy/node_modules/.pnpm/vite-node@2.1.9_@types+node@22.13.1_sass-embedded@1.83.4/node_modules/vite-node/node_modules:/Users/moisesgomes/Desktop/bolt.diy - cópia/bolt.diy/node_modules/.pnpm/vite-node@2.1.9_@types+node@22.13.1_sass-embedded@1.83.4/node_modules:/Users/moisesgomes/Desktop/bolt.diy - cópia/bolt.diy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../vite-node/vite-node.mjs" "$@"
fi
